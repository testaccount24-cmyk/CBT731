//SBGOLOBU JOB (CCMVS),ASMLINK,
// NOTIFY=&SYSUID,
// CLASS=A,MSGCLASS=X,COND=(1,LT)
//* ------------------------------------------------------------- */
//*   ASSEMBLE AND LINKEDIT THE LOADTEST PROGRAM                  */
//* ------------------------------------------------------------- */
//*   THE ALIAS OF LOADTN WILL JUST RUN THE POST-PROCESSING       */
//*   TEST PARM REPORT PROGRAM LOADTN, USING OPTIONAL DD NAMES    */
//*   SYSPRNT2 AND TESTOUT2.                                      */
//* ------------------------------------------------------------- */
//ASMH EXEC PGM=ASMA90,PARM=(OBJECT,NODECK,NOESD,NORLD,NOXREF,FLAG(5))
//SYSLIB   DD DISP=SHR,DSN=SYS1.MACLIB
//         DD DISP=SHR,DSN=SYS1.MODGEN
//SYSIN    DD DISP=SHR,DSN=SBGOLOB.B.ASM(LOADTEST)
//SYSUT1   DD UNIT=VIO,SPACE=(CYL,2)
//SYSPRINT DD SYSOUT=*
//SYSLIN   DD UNIT=VIO,SPACE=(TRK,1),DISP=(,PASS),DCB=BLKSIZE=3200,
//            DSN=&&TEMP
//*
//LKED   EXEC  PGM=HEWL,
//       PARM='NCAL,MAP,LIST,LET'
//SYSUT1   DD  UNIT=SYSDA,SPACE=(2048,(200,20))
//SYSPRINT DD  SYSOUT=*
//SYSLIB   DD  DISP=SHR,DSN=SYS1.W$$.LINKLIB
//SYSLMOD  DD  DISP=SHR,DSN=SYS1.W$$.LINKLIB
//OBJECT   DD  DISP=(OLD,DELETE),DSN=&&TEMP
//SYSLIN   DD  *
 INCLUDE OBJECT
 SETSSI  CB498731
 SETCODE AC(1)
 ALIAS   LOADTN
 ENTRY   LOADTEST
 NAME    LOADTEST(R)
